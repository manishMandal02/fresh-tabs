{
  "name": "fresh-tabs",
  "version": "0.0.1",
  "description": "Manage tabs like a pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manishMandal02/fresh-tabs.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@lexical/code": "^0.14.2",
    "@lexical/link": "^0.14.2",
    "@lexical/list": "^0.14.2",
    "@lexical/markdown": "^0.14.2",
    "@lexical/react": "^0.14.2",
    "@lexical/rich-text": "^0.14.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "add": "^2.0.6",
    "chrono-node": "^2.7.5",
    "construct-style-sheets-polyfill": "3.1.0",
    "emoji-picker-react": "^4.6.0",
    "framer-motion": "^10.18.0",
    "jotai": "^2.6.1",
    "lexical": "^0.14.2",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-frame-component": "^5.2.6",
    "react-hook-form": "^7.51.1",
    "react-icons": "^4.12.0",
    "react-tooltip": "^5.25.1",
    "victory": "^36.9.1",
    "webextension-polyfill": "0.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.251",
    "@types/node": "20.8.10",
    "@types/react": "18.2.37",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "18.2.14",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "^10.4.16",
    "chokidar": "3.5.3",
    "clsx": "^2.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.53.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "15.0.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "react-geiger": "^1.1.0",
    "rollup": "4.3.0",
    "sass": "1.69.5",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.0",
    "tailwindcss-all": "^0.0.2",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vitest": "^0.34.6",
    "ws": "8.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@8.9.2"
}
